{
  "name": "create-container-component",
  "version": "0.2.1",
  "description": "Tool to generate React container and/or components from the terminal",
  "bin": {
    "create-component-app": "./dist/index.js"
  },
  "repository": "https://github.com/davidlewallen/create-container-component.git",
  "author": "David Lewallen <lewallen.david@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint src test",
    "build": "babel src -d dist",
